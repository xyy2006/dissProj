@Article{Kamatani2010,
  Title                    = {Genome-wide association study of hematological and biochemical traits in a Japanese population.},
  Author                   = {Kamatani, Yoichiro and Matsuda, Koichi and Okada, Yukinori and Kubo, Michiaki and Hosono, Naoya and Daigo, Yataro and Nakamura, Yusuke and Kamatani, Naoyuki},
  Journal                  = {Nat Genet},
